{
  "name": "studyflow-backend",
  "version": "1.0.0",
  "description": "Backend API for StudyFlow - AI-powered study notes sharing platform",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "db:sync": "node -e \"import('./src/config/database.js').then(m => m.default.sync({ force: false }))\"",
    "db:reset": "node -e \"import('./src/config/database.js').then(m => m.default.sync({ force: true }))\""
  },
  "keywords": [
    "studyflow",
    "notes",
    "ai",
    "education"
  ],
  "author": "Equipo 3",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.20.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "pdf-parse": "^2.2.2",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  }
}
